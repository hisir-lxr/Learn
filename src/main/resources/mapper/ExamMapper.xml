<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="online.mctom.learn.mapper.ExamMapper">

    <resultMap id="BaseResultMap" type="online.mctom.learn.entity.Exam">
            <id property="examId" column="exam_id" jdbcType="INTEGER"/>
            <result property="examName" column="exam_name" jdbcType="VARCHAR"/>
            <result property="courseId" column="course_id" jdbcType="INTEGER"/>
            <result property="examScore" column="exam_score" jdbcType="FLOAT"/>
            <result property="examTime" column="exam_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        exam_id,exam_name,course_id,
        exam_score,exam_time
    </sql>
    <insert id="insert">
        INSERT INTO web.exams (exam_name,course_id,
                           exam_score,exam_time)
        value (#{examName}, #{courseId}, #{examScore}, #{examTime})
    </insert>
</mapper>
